<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.nobelz.employee">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".admin" />
        <activity android:name=".Message" />
        <activity android:name=".GroupTask" />
        <activity android:name=".Composemsg" />
        <activity android:name=".Inboxmsg" />
        <activity android:name=".Sentmsg" />
        <activity android:name=".Adminmanage" />
        <activity android:name=".Location" />
        <activity android:name=".Leader" />
        <activity android:name=".Member" />
        <activity android:name=".Ledtask" />
        <activity android:name=".Memtask"></activity>
    </application>

</manifest>